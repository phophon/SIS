generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource DS {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model student {
  addressstatus    String?
  airtableid       String? @default("NULL")
  canvasid         String? @default("NULL")
  emergencycontact String? @default("NULL")
  firstname        String? @default("NULL")
  gender           gen?
  lastname         String? @default("NULL")
  phonenumber      String? @default("NULL")
  photo            String? @default("NULL")
  primaryemail     String? @default("NULL")
  region           String? @default("NULL")
  secondaryemail   String? @default("NULL")
  uuid             Int     @id
}

enum gen {
  female
  male
}
